{
  "compilerOptions": {
    "checkJs": true,
    "allowJs": true,
    "module": "commonjs",
    "target": "ES2024",
    "moduleResolution": "node",
    "types": ["node"],
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strict": false,
    "typeRoots": ["./node_modules/@types"]
  },
  "typeAcquisition": {
    "enable": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
